{
  "name": "smart-logger-ts",
  "version": "1.0.5",
  "description": "Flexible and powerful logger for TypeScript projects with visual and console modes",
  "main": "./dist/main.umd.js",
  "module": "./dist/main.es.js",
  "types": "./dist/main.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/main.es.js",
