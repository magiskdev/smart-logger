    "typescript",
    "console",
    "visual-logger"
  ],
  "author": "Kushnikov Alexandr",
  "license": "MIT",
  "devDependencies": {
    "sass-embedded": "^1.80.6",
    "typescript": "~5.6.2",
    "vite": "^5.4.10",
    "vite-plugin-dts": "^4.3.0",
    "vite-plugin-legacy": "^2.1.0",
    "vite-plugin-lib-inject-css": "^2.1.1"
  }
}
